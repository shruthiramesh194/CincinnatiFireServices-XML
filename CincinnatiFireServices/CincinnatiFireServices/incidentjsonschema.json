{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "array",
  "items": {
    "$ref": "#/definitions/IncidentElement"
  },
  "definitions": {
    "IncidentElement": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "address_x": {
          "type": "string"
        },
        "latitude_x": {
          "type": "string"
        },
        "longitude_x": {
          "type": "string"
        },
        "agency": {
          "type": "string"
        },
        "create_time_incident": {
          "type": "string",
          "format": "date-time"
        },
        "disposition_text": {
          "type": "string"
        },
        "event_number": {
          "type": "string",
          "minLength": 16,
          "maxLength": 16,
          "pattern": "^[a-z]{4}[0-9]{12}$"
        },
        "incident_type_id": {
          "type": "string"
        },
        "incident_type_desc": {
          "type": "string"
        },
        "neighborhood": {
          "type": "string"
        },
        "arrival_time_primary_unit": {
          "type": "string",
          "format": "date-time"
        },
        "beat": {
          "type": "string"
        },
        "closed_time_incident": {
          "type": "string",
          "format": "date-time"
        },
        "dispatch_time_primary_unit": {
          "type": "string",
          "format": "date-time"
        },
        "cfd_incident_type": {
          "type": "string"
        },
        "cfd_incident_type_group": {
          "type": "string"
        },
        "community_council_neighborhood": {
          "type": "string"
        }
      },
      "required": [
        "create_time_incident",
        "event_number"
      ],
      "title": "IncidentElement"
    }
  }
}
